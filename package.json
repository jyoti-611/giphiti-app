{
  "name": "shopify-app-node",
  "private": true,
  "scripts": {
    "build": "npm run build:client",
    "build:client": "vite build --outDir dist/client",
    "debug": "node --inspect-brk server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch ./server",
    "prepare": "husky install",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production node server/index.js",
    "start": "npm run serve",
    "test": "vitest --reporter=verbose"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@reduxjs/toolkit": "^1.8.5",
    "@shopify/app-bridge": "^2.0.22",
    "@shopify/app-bridge-react": "^2.0.26",
    "@shopify/app-bridge-utils": "^2.0.26",
    "@shopify/polaris": "^9.22.0",
    "@shopify/shopify-api": "^4.0.0",
    "@vitejs/plugin-react": "1.3.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "firebase": "^9.9.4",
    "formik": "^2.2.9",
    "graphql": "^16.5.0",
    "joi": "^17.6.1",
    "joi-tz": "^4.1.1",
    "moment-timezone": "^0.5.37",
    "mongoose": "^6.7.2",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-csv": "^2.2.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.0",
    "react-scripts": "^5.0.1",
    "react-tabulator": "^0.18.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.1",
    "sequelize": "^6.23.0",
    "serve-static": "^1.14.1",
    "uuid": "^8.3.2",
    "vite": "^2.9.8",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.3",
    "vitest": "^0.10.5"
  }
}
